{
  "name": "cook-n-shop-2",
  "version": "2.0.0",
  "description": "New version of Cook-N-Shop using Fullstack with Vue, GraphQl, MySql, Express",
  "main": "server.js",
  "scripts": {
    "setUpTest": "npm run resetTest && cross-env NODE_ENV=test npm run testMigrate",
    "testMigrate": "sequelize db:migrate && sequelize db:seed:all --debug",
    "resetTest": "cross-env NODE_ENV=test sequelize db:seed:undo:all && cross-env NODE_ENV=test sequelize db:migrate:undo:all --debug",
    "test": "cross-env NODE_ENV=test mocha test/integration/server.spec.js",
    "testAll": "cross-env NODE_ENV=test mocha test/**/**.spec.js",
    "start": "node src/server.js",
    "dev": "nodemon NODE_ENV=develop src/server.js",
    "debug": "node --nolazy src/server.js"
  },
  "keywords": [
    "Vue",
    "GraphQl",
    "Express",
    "MySql"
  ],
  "author": "Anh Dao",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "fs": "0.0.1-security",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.4",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "path": "^0.12.7",
    "proxyquire": "^2.1.3",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.2.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "sequelize-cli": "^6.2.0",
    "sequelize-mock": "^0.10.2",
    "sequelize-test-helpers": "^1.3.2",
    "sinon": "^9.2.0",
    "sinon-chai": "^3.5.0",
    "supertest": "^6.0.0"
  },
  "resolutions": {
    "graphql": "^0.13.0",
    "**/graphql": "^0.13.0"
  }
}
